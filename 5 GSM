clc;
clear all;
close all
a=[5 5 5 5;6 6 6 6;7 7 7 7;8 8 8 8;9 9 9 9;10 10 10 10];
b=[20 20 20 20;21 21 21 21;22 22 22 22;23 23 23 23;24 24 24 24;25 25 25 25];
[p,q]=size(a)
disp('The first data');
disp(a)
disp('The second data');
disp(b)
disp('Output');
for i=1:p
  c=[a(i,1:q) b((7-i),1:q)];
  disp(c)
  end
